  </alias>
  <alias
   match="autokill (?P&lt;target&gt;.+)$"
   enabled="y"
   regexp="y"
   send_to="12"
   sequence="50"
  >
  <send>world.setVariable("varAutokillPre","ss3");

world.setVariable("varAutokillDone","whisper");
world.setVariable("varAutokillCount",0);
world.setVariable("varAutokillStart",1);
var ts="%&lt;target&gt;".split("+");
var cmd="";
for (var i=0;i&lt;ts.length;i++){
cmd=cmd+"kill "+ts[i]+";";
}
cmd=cmd+"ssAutokillPre";
world.enableGroup("autokill",1);
world.execute(cmd);
</send>
  </alias>

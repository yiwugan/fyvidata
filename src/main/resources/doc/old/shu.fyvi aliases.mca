  <alias
   match="tasklocate"
   group="task"
   send_to="12"
   sequence="100"
  >
  <send>world.enableGroup("taskmove",0);
var loc=world.getVariable("varTaskLastLocation");
var objs=world.arrayGet("tasklocationmap",loc);
if (objs==null || objs==""){
//not task location
//world.note("not task location:"+loc);
world.execute("tasklocatedone");
}else {
 world.enableGroup("tasklocate",1);
 var nlist=world.arrayExportKeys("tasknpclist",",").split(",");
// world.note("npc list:"+world.arrayExportKeys("tasknpclist",","));
 var act="";
 var lastobjects="";
 var n=0;
 for (n=0;n&lt;nlist.length;n++){
//  world.note("npc:"+nlist[n]);
  if (nlist[n].indexOf(" 1")&gt;=0){
    var newname=nlist[n].replace(" 1","");
    world.arrayDeleteKey("tasknpclist",nlist[n]);
    world.arraySet("tasknpclist",newname,newname);
  }
  if (nlist[n].indexOf("corpse")&gt;=0 || nlist[n].indexOf("skeleton")&gt;=0){
   world.arrayDeleteKey("tasknpclist",nlist[n]);
   act=act+"look "+nlist[n].replace(" 1","")+";";
   lastobjects=lastobjects+nlist[n].replace(" 1","")+";";
  }
 }
// world.note("act:"+act);
 if (objs=="null"){
  //task location, no object
  world.setVariable("varTaskLastObjects",lastobjects.substring(0,lastobjects.length-1));
  if (act!="") world.execute(act.substring(0,act.length-1));
 }else{
  //task location with object
  world.setVariable("varTaskLastObjects",lastobjects+objs);
  world.enableGroup("tasklocate",1);
  var objlist=objs.split(";");
  var i=0;
  for (i=0;i&lt;objlist.length;i++){
   act=act+"look "+objlist[i]+";";
  }
  world.execute(act.substring(0,act.length-1));
 }
 world.execute("ask");
}

</send>

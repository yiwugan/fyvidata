  <alias
   match="ssAutokill"
   group="autokill"
   send_to="12"
   sequence="90"
  >
  <send>local now=parseInt(new Date().getTime()/1000)
Setvariable("varLastPerformTime",now)
local tg=Getvariable("varKillTarget")
if tg=="abortkill" then
Setvariable("varAutoKillAbort","yes")
EnableGroup("autokill-perform",0)
EnableGroup("autokill-busy",0)
EnableGroup("autokill-abort",1)
end
local c=GetVariable("varAutoKillCount")
if c&gt;1 then
--skip first perform because we already have pre-perform
local i=c/10;
i=i.. "";
local p=i.indexOf(".")
local d=0;
if p&gt;0 then
i=i.substr(p+1,1)
d=i;
 else 
d=0;
end
Note("---&gt; autokill count=" .. c.. " perform=" .. d)
local performs=Getvariable("varAutokillList").split(",")
Execute(generateAutokillPerform(performs[d]))

local abort=Getvariable("varAutokillAbortCount")
if abort==nil or abort==undefined then
abort=200;
Setvariable("varAutokillAbortCount",abort)
 else 
abort=parseInt(abort)
end

if c&gt;=abort then
Note("---&gt; abort auto kill now")
Execute("burn")
end

end</send>

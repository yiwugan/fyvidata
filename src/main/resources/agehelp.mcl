if t==nil or t=="" then
Note("---&gt; first time, go fight now")
Execute("gsCheckDuguClear")
 else 
local nowSec=new Date().getTime() / 1000;
local diff=nowSec-t;
if diff&gt;3600 then
Note("---&gt; fight time reach, go fight now")
Execute("gsCheckDuguClear")
 else 
local min=60-parseInt(diff/60)
Note("---&gt; remain fight time " .. min.. " minutes")

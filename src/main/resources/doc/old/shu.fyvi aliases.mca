  >
  <send>world.setVariable("varAutokillPre","ss2");

world.setVariable("varAutokillDone","questKillDone");
world.setVariable("varAutokillCount",0);
world.setVariable("varAutokillStart",1);
world.setvariable("varQuestKillCorpseAmount",0);
world.enableGroup("quest-kill",1);

  <alias
   match="ssAutokill"
   group="autokill"
   send_to="12"
   sequence="90"
  >
  <send>
var c=1;
c=world.getVariable("varAutoKillCount");
var i=c/5;
i=i+"";
var p=i.indexOf(".");
var d=0;
if (p&gt;0){
i=i.substr(p+1,1);
d=i/2;
}else{
d=0;
}
world.note("---&gt; autokill count="+c+" perform="+d);
if (d==0)
world.execute(generateAutokillPerform(world.getVariable("varAutokill1")));
if (d==1)
world.execute(generateAutokillPerform(world.getVariable("varAutokill2")));
if (d==2)
world.execute(generateAutokillPerform(world.getVariable("varAutokill3")));
if (d==3)
world.execute(generateAutokillPerform(world.getVariable("varAutokill4")));
if (d==4)
world.execute(generateAutokillPerform(world.getVariable("varAutokill5")));

//else
//world.execute(world.getVariable("varSS1"));

//if (c==21){
//world.execute("sp11;sp12;sp13");
//}

//if (c&gt;=600){
//world.enableGroup("autokill",0);
//world.Disconnect();
//world.connect();
//}

</send>

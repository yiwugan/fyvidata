  <alias
   match="^gg0 (?P&lt;item&gt;.*)$"
   group="quest-return"
   regexp="y"
   send_to="12"
   sequence="30"
  >
  <send>if ("%&lt;item&gt;".indexOf(" ")&lt;0 
   || ("%&lt;item&gt;".indexOf(" ")&gt;0 
      &amp;&amp; "%&lt;item&gt;".split(" ").length==2 
      &amp;&amp; endWithNumber("%&lt;item&gt;")
      ) 
){
world.send("get "+'%&lt;item&gt;'+" from "+world.GetVariable("varMyBox2"));
}else{
world.send("get "+'%&lt;item&gt;'+" from "+world.GetVariable("varMyBox") );
}
</send>
  </alias>
  <alias
   match="^pp0 (?P&lt;item&gt;.*)$"
   group="quest-return"
   regexp="y"
   send_to="12"
   sequence="30"
  >
  <send>var b1=world.getvariable("varMyBox");
var b2=world.getvariable("varMyBox2");

if ("%&lt;item&gt;"=="all"){
world.execute("put all in "+ b2+";get "+b1+" from "+b2);
}else if ("%&lt;item&gt;".indexOf(" ")&lt;0 ){
world.send("put "+'%&lt;item&gt;'+" in "+b2);
}else{
world.send("put "+'%&lt;item&gt;'+" in "+b1 );
}</send>
  </alias>

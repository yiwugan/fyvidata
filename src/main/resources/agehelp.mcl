  <alias
   match="goNextQuest"
   group="quest-action"
   send_to="12"
   sequence="100"
  >
  <send>//Execute("pp silver")

local q=listQuestActiveQueue.pop()
Note("---&gt; quest target:" .. q)

if q==undefined then
Note("---&gt; no workable quest in queue, wait for 20 seconds")
Doafterspecial(20,"resumeQuest",10)
local qc=GetVariable("varQuestCancelCount")
Note("---&gt; current quest cancel count " .. qc)
qc=parseInt(qc)+1;
if qc&lt;4 then
Execute("tt team-quest-cancel")
SetVariable("varQuestCancelCount",qc)
Doafterspecial(5,"tt quest-return-start",10)
end

 else 

local path=queryQuestPath(q)
if path==nil then
--quest path is nil, skip it
Note("---&gt; quest path not found, skip " .. q)
SetVariable("varQuestCurrentTarget",q)
Execute("questDoneOrGone")
Doafterspecial(1,"goNextQuest",10)
 else 
Note("---&gt; quest path:" .. path)
if path.indexOf("Kill")&gt;=0 then
SetVariable("varQuestReturnItem","corpse")
 else 
local iename=queryQuestItemName(q)
SetVariable("varQuestReturnItem",iename)
end
SetVariable("varQuestCurrentTarget",q)
--check if need to clear shaolin mark
local needClearShaolin=0;
if path.indexOf("-clearShaolin")&gt;0 then
needClearShaolin=1;
path=path.replace("-clearShaolin","")
end
initActionQueue()
--check if need team fly or fly
if path.indexOf("goo")==0 then
pushSingleAction("out;fy-chantu")
endelse if path.indexOf("get corpse;goo")==0 then
pushSingleAction("get corpse;out;fy-chantu")
 else 
path="out;" .. path;
end
generateQuestActionMove(path)
if needClearShaolin==1)
 generateQuestClearShaolinMove()
else
 generateQuestPostMove()
startAction(false)
end

end</send>

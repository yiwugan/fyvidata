  <alias
   match="tasklocate"
   group="task"
   send_to="12"
   sequence="100"
  >
  <send>EnableGroup("taskmove",0)
local loc=GetVariable("varTaskLastLocation")
local objs=ArrayGet("tasklocationmap",loc)
if objs==nil or objs=="" then
--not task location
--Note("not task location:" .. loc)
Execute("tasklocatedone")
endelse {
 EnableGroup("tasklocate",1)
 local nlist=ArrayExportKeys("tasknpclist",",").split(",")
-- Note("npc list:" .. ArrayExportKeys("tasknpclist",","))
 local act="";
 local lastobjects="";
 local n=0;
 forn=0;n&lt;nlist.length;n++ then
-- Note("npc:" .. nlist[n])
 if nlist[n].indexOf(" 1")&gt;=0 then
  local newname=nlist[n].replace(" 1","")
  ArrayDeleteKey("tasknpclist",nlist[n])
  ArraySet("tasknpclist",newname,newname)
 end
 if nlist[n].indexOf("corpse")&gt;=0 or nlist[n].indexOf("skeleton")&gt;=0 then
  ArrayDeleteKey("tasknpclist",nlist[n])
  act=act.. "look " .. nlist[n].replace(" 1","").. ";";
  lastobjects=lastobjects+nlist[n].replace(" 1","").. ";";
 end
 end
-- Note("act:" .. act)
 if objs=="nil" then
 //task location, no object
 SetVariable("varTaskLastObjects",lastobjects.substring(0,lastobjects.length-1))
 if act~="") Execute(act.substring(0,act.length-1))
 else 
 //task location with object
 SetVariable("varTaskLastObjects",lastobjects+objs)
 EnableGroup("tasklocate",1)
 local objlist=objs.split(";")
 local i=0;
 fori=0;i&lt;objlist.length;i++ then
  act=act.. "look " .. objlist[i].. ";";
 end
 Execute(act.substring(0,act.length-1))
 end
 Execute("ask")
end

</send>

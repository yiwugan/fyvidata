local start="%&lt;start&gt;";
local locs=findLocationsByArea(area)
initActionQueue()
local i=0;
fori=0;i&lt;locs.length;i++ then
--Note("path id:" .. locs[i].. "  , path:" .. path)
local path=queryMovePathById(locs[i])
local path2=parsePathActionForMission(path,area)
Note("---&gt; path for " .. locs[i].. ":" .. path2)
if path2.indexOf(start)==0 then
generateMissionActionMove(path2)
pushSingleAction("reachLocation")
--pushSingleAction("umk")
--pushSingleAction("wd;whisper")
end
end
startAction(true)</send>
